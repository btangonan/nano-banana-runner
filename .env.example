# Node environment
NODE_ENV=development

# Logging
LOG_LEVEL=info

# Google Cloud settings for Vertex AI
# Set your project ID here or use: gcloud config set project <PROJECT_ID>
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=us-central1

# Provider selection
NN_PROVIDER=gemini

# Output configuration
NN_OUT_DIR=./artifacts

# Optional GCS bucket for production file storage
# GCS_BUCKET=your-bucket-name

# GUI settings
GUI_PORT=4173
GUI_HOST=127.0.0.1

# Performance settings
NN_CONCURRENCY=2
NN_MAX_PER_IMAGE=50

# Feature flags
NN_ENABLE_GCS=false
NN_ENABLE_CACHE=true