{
  "name": "nn",
  "version": "0.1.0",
  "description": "Nano Banana Runner - Terminal Image Analyzer & Prompt Remixer",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "nn": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "build:web": "vite build",
    "build:all": "pnpm build && pnpm build:web",
    "dev": "tsup --watch",
    "dev:web": "vite",
    "test": "vitest",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "validate": "node scripts/validate.mjs",
    "bench": "node scripts/bench.mjs",
    "nn": "node dist/cli.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@google-cloud/vertexai": "^1.9.0",
    "@google-cloud/storage": "^7.14.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "csv": "^6.3.11",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/static": "^8.0.2",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "sharp": "^0.33.5",
    "undici": "^6.21.0",
    "uuid": "^11.0.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "gitleaks": "^8.22.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "supertest": "^7.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vitest": "^2.1.8"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nn"
  },
  "keywords": [
    "image-analysis",
    "prompt-engineering",
    "vertex-ai",
    "gemini",
    "cli"
  ],
  "author": "",
  "license": "MIT"
}